#include <iostream>
using namespace std;

int rel[8][3];
rel[0] = {1,2,4};
rel[1] = {0,3,5};
rel[2] = {0,3,6};
rel[3] = {1,2,7};
rel[4] = {0,5,6};
rel[5] = {1,4,7};
rel[6] = {2,4,7};
rel[7] = {3,5,6};

int main(){
	int n,m;
	cin >> n;
	while(n--){
		cin >> m;
		for (int i=0;i<m;i++) for (int j=0;j<8;j++) cin >> a[i][j];
		int f[m][8]; // f[i][j] = how voter number i will decide that what happens to number j - at the end!
		for (int i=0;i<8;i++){
			int t0 = a[m-1][rel[i][0]];
			int t1 = a[m-1][rel[i][1]];
			int t2 = a[m-1][rel[i][2]];
			if (t0 <= t1 && t0 <= t2) f[m-1][i] = rel[i][0];
			else if (t1 <= t2) f[m-1][i] = rel[i][1];
			else f[m-1][i] = rel[i][2];
		}

	}
	return 0;
}
