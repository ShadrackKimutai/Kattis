import java.math.BigInteger;
import java.util.Scanner;


public class j {
	public static void main(String[] args) {
		BigInteger T,a,m,b,n;
		Scanner in = new Scanner(System.in);
		T = in.nextInt();
		while (T--){
			cin >> a >> n >> b >> m;
			ll K = n*m;
			ll x = (((a*m)%K)*mod_inv(m,n))%K;
			x += (((b*n)%K)*mod_inv(n,m))%K;		
			x %= K;
			cout << x << " " << K << endl;
		}
		
		System.out.println();
	}
}
