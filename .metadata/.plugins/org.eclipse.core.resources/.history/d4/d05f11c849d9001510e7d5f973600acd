#include <iostream>
#include <vector>
#include <stack>
using namespace std;

int main(){
	int t,n,m;
	cin >> t;
	while (t--){
		cin >> n >> m;
		vector<int> g[n];
//		for (int i=0;i<n;i++) for (int j=0;j<n;j++) g[i][j] = 0;
		for (int i=0;i<m;i++){ // make graph
			cin >> u-- >> v--;
			g[u].push_back(v);
		}
		// DFS

	}
	return 0;
}
