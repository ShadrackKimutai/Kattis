#include <iostream>
#include <vector>
#include <stack>
#include <cstring>
using namespace std;

const int N = 20000;

bool marked[N];
stack<int> s;
vector<int> g[N],gi[N];

int main(){
	int t,n,m;
	cin >> t;
	while (t--){
		cin >> n >> m;
//		for (int i=0;i<n;i++) for (int j=0;j<n;j++) g[i][j] = 0;
		for (int i=0;i<m;i++){ // make graph
			cin >> u-- >> v--;
			g[u].push_back(v);
			gi[v].push_back(u);
		}
		// DFS
		memset(marked,false,sizeof(marked));



	}
	return 0;
}
